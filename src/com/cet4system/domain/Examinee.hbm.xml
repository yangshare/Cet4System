<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cet4system.domain">

	<class name="Examinee" table="examinee">
		<id name="candidateNo"/>
		<property name="name"/>
		<property name="sex"/>
	    <property name="examineeOfSchool"/>
	    <property name="image"/>
	    <property name="writingGrade"/>
	    <property name="listeningGrade"/>
	    <property name="readingGrade"/>
	    <property name="comprehensivingGrade"/>
	    <property name="total"/>
	 
	</class>

</hibernate-mapping>